<?xml version="1.0"?>
<robot name="zoe">

  <link name="base_link"></link>

  <!-- it is pandora, but named as velodyne for notation-->
  <link name="velodyne"> </link>
  <joint name="velodyne_joint" type="fixed">
    <parent link="base_link" />
    <child link="velodyne" />
    <origin xyz="1.18 0 1.41" rpy="0 0 1.5707" />
  </joint>

  <link name="span"> </link>
  <joint name="span_joint" type="fixed">
    <parent link="base_link" />
    <child link="span" />
    <origin xyz="-0.023 0.005 0.200" rpy="0 0 0" />
  </joint>

</robot>